#hierCon td span {
	font-size:18px;
}
#mask {
	height: 100%;
	width: 100%;
	position: fixed;
	display: none;
	opacity: 0.7;
	background: #fff;
	z-index: 100;
	top: 0;
	left: 0;
}
#hierCon td {
	padding-left:5px;
}
#hierCon td span{
	cursor: pointer;
    display: inline-block;
    text-align: center;
    width: 15px;
}
#infoDiv {
	height: 200px;
	overflow-y: auto;
}

